{
    "name": "netteam/ddd-bundle",
    "type": "symfony-bundle",
    "description": "NetTeam DDD Bundle",
    "authors": [
        {
            "name": "NetTeam Solutions",
            "homepage": "http://netteam.pl"
        }
    ],
    "repositories": [
        { "type": "composer", "url": "http://composer.nts/" }
    ],
    "require": {
        "php": ">=5.3.2",
        "netteam/ddd": "~1.0@dev",
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "symfony/validator": "~2.1",
        "symfony/form": "~2.1",
        "twig/twig": "~1.1",
        "mockery/mockery": ">=0.7.2@dev"
    },
    "autoload": {
        "psr-0": { "NetTeam\\Bundle\\DDDBundle": "" }
    },
    "target-dir": "NetTeam/Bundle/DDDBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
